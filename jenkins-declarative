pipeline{
    agent any
    stages{
        stage("Git Checkout"){
            steps{
git branch: 'main', credentialsId: 'github', url: 'https://github.com/sujeetkr7436/shoppingcartmvc.git'
}
        }
        
        stage{
            steps("Maven Build"){
                sh 'mvn clean package'
            }
        }
        
    }
    
}
